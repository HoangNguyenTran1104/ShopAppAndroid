<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bignerdranch.android.shopapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShopApp">
        <activity android:name=".GoogleMap"></activity>
        <activity android:name=".CheckoutSuccessful" />
        <activity android:name=".LoginSuccessful" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login" />
        <activity android:name=".SunglassesProduct" />
        <activity android:name=".KidProduct" />
        <activity android:name=".MenProduct" />
        <activity android:name=".CartShop" />
        <activity android:name=".ProductDetail" />
        <activity android:name=".WonmenProduct" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>